{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "moduleResolution": "node",
    "types": ["@nuxt/types", "@types/node"],
    "paths": {
      "#app": ["./.nuxt"],
      "#components": ["./components"],
      "#composables": ["./composables"],
      "#plugins": ["./plugins"]
    },
    "baseUrl": ".",
    "module": "esnext",
  },
  "exclude": ["node_modules", ".nuxt", "dist"]
}
